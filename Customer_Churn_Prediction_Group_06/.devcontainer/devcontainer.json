{
  "name": "ML Practical Environment",
  "build":{
    "dockerfile": "../dockerfile",
    "context": ".."
  },
  "image": "ubuntu:22.04",
  "runArgs": [
    "--privileged",
    "--network=host"
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "eamodio.gitlens",
        "ms-toolsai.jupyter",
        "ms-azuretools.vscode-docker",
        "ms-vscode-remote.remote-containers"
      ]
    }
  },
  "forwardPorts": [8888],
  "remoteUser": "root"
}
